<Application x:Class="VolleyballApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:VolleyballApp">
    <Application.Resources>
        <ResourceDictionary>
            <!-- Brushes -->
            <SolidColorBrush x:Key="PrimaryBrush" Color="#f6e1b8"/>
            <SolidColorBrush x:Key="SecondaryBrush" Color="#c65f63"/>
            <SolidColorBrush x:Key="TertiaryBrush" Color="#84577c"/>
            <SolidColorBrush x:Key="FourthBrush" Color="#333644"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
